{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "plugins": [],
  "rules": {
    "react/jsx-filename-extension": "off",
    "linebreak-style": "off",
    "max-len": ["error", {
      "code": 120,
      "ignoreComments": true,
      "ignoreRegExpLiterals": true,
      "ignoreUrls": true,
      "tabWidth": 2
    }]
  }
}